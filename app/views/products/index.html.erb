<h1> Products <h1>
  <p><%= link_to 'New Product', new_product_path %></p>
  <tbody>
<% @products.each do |product| %>
<tr>
  <h2><%= product.name %></h2>
  <p><%= product.price %></p>
  <%= link_to 'Show Product', product_path(product) %>
  <%= link_to 'edit', edit_product_path(product) %>
  <%= link_to 'destroy', "#" %>
<% end %>

